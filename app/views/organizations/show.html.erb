<div class="col-md-8">
  <ul class="list-unstyled">
    <h2>Repositories</h2>
    <li>
       <% @organization.repositories.each do |repository| %>
        <%= link_to repository.name, organization_repository_path(organization_id: repository.organization_id, id: repository.id) %>
       <% end %>
    </li>
  </ul>
</div>
