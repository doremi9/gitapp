<% provide(:title, "Home") %>

<% if !current_user %>
  <div class="center jumbotron">
    <h1>Welcome to GitApp</h1>
    <%= link_to "Sign up with GitHub!", '/auth/github', class: "btn btn-lg btn-primary" %>
  </div>
<% else %>
  <div class="activities">
    <h1>Recent activities</h1>
  </div>
<% end %>